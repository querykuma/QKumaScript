print "# test - for/function return";
println;

var fun = function() {
    var i = 1;
    var sum = 0;

    for (i=0; i<50; i=i+1) {
        print i;
        sum = sum + i;
        if (i==5) {
            println;
            return sum;
        } else {
            print ", ";
        }
    }

    return -1;
};

var sum = fun();
print "sum = ";
print sum;
println;
